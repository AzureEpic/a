Tab7:AddButton({
	Name = "Make all of your parts EXTREMELY heavy (WIP)",
	Callback = function()

		for _, des in ipairs(workspace[PlayerToysFolder]:GetDescendants()) do
			if des:IsA("BasePart") then
			local mesh = Instance.new("SpecialMesh")
			mesh.Parent = des
			mesh.MeshType = Enum.MeshType.FileMesh
				mesh.MeshId = "rbxassetid://97511242213020"
				
			end
		end
	end    
})


Tab7:AddButton({
Name = "grab every player in the server"

Callback = function()

for i, pl in game.Players:GetPlayers() do

local args = {
    [1] = pl.Character.HumanoidRootPart,
    [2] = CFrame.new()
}

game:GetService("ReplicatedStorage").GrabEvents.CreateGrabLine:FireServer(unpack(args))





end
end

})


-- Initialize the Orion Library UI
OrionLib:Init()
